#include <stdio.h>

int main() {
    FILE *f;
    char line[200];   

    f = fopen("test.txt", "r");   

    if (f == NULL) {
        printf("File not found!");
        return 0;
    }

    while (fgets(line, sizeof(line), f)) {   
        printf("%s", line);                  
    }

    fclose(f);

    return 0;
}
